,UAAA,KAAA,CAAA,KAAA,CAAA,QAAA,CAAA,aAAA,CAAA,WAAA;AACA;;AACA,QAAA,QAAA;AACA;AACA,KATA;;AAUA,QAAA,aAAA,GAAA,SAAA,aAAA,CAAA,IAAA,EAAA,KAAA,EAAA,QAAA,EAAA;AACA,UAAA,KAAA,KAAA,EAAA,EAAA;AACA,QAAA,QAAA,CAAA,IAAA,KAAA,CAAA,SAAA,CAAA,CAAA;AACA,OAFA,MAEA,IAAA,KAAA,KAAA,KAAA,CAAA,QAAA,CAAA,IAAA,EAAA;AACA,QAAA,QAAA,CAAA,IAAA,KAAA,CAAA,YAAA,CAAA,CAAA;AACA,OAFA,MAEA;AACA,QAAA,QAAA;AACA;AACA,KARA;;AASA,WAAA;AACA,MAAA,MAAA,EAAA,QAAA,CAAA,YAAA,CAAA,MAAA,CADA;AAEA,MAAA,QAAA,EAAA,EAFA;AAGA,MAAA,QAAA,EAAA;AACA,QAAA,IAAA,EAAA,EADA;AAEA,QAAA,SAAA,EAAA,EAFA;AAGA,QAAA,QAAA,EAAA;AAHA,OAHA;AAQA,MAAA,KAAA,EAAA;AACA,QAAA,IAAA,EAAA,CAAA;AAAA,UAAA,SAAA,EAAA,YAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA,CADA;AAEA,QAAA,SAAA,EAAA,CAAA;AAAA,UAAA,SAAA,EAAA,aAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA,CAFA;AAGA,QAAA,QAAA,EAAA,CAAA;AAAA,UAAA,SAAA,EAAA,aAAA;AAAA,UAAA,OAAA,EAAA;AAAA,SAAA;AAHA;AARA,KAAA;AAcA,GAhDA;AAkDA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,wBACA;AAEA,UAAA,KAAA,QAAA,CAAA,IAAA,CAAA,MAAA,GAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,WAAA,CAAA;AACA,OAFA,MAEA;AACA;AACA,YAAA,IAAA,GAAA,IAAA;AACA,QAAA,OAAA,CAAA;AACA,UAAA,MAAA,EAAA,MADA;AACA;AACA,UAAA,GAAA,EAAA,WAFA;AAEA;AACA,UAAA,IAAA,EAAA;AACA;AACA,YAAA,QAAA,EAAA,KAAA,QAAA,CAAA,QAFA;AAGA,YAAA,QAAA,EAAA,MAAA,CAAA,GAAA,CAAA,KAAA,QAAA,CAAA,IAAA,CAHA;AAIA,YAAA,MAAA,EACA;AALA;AAHA,SAAA,CAAA,CAUA,IAVA,CAUA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,IAAA,SAAA,EAAA;AACA,YAAA,KAAA,CAAA,OAAA,CAAA;AACA,YAAA,IAAA,CAAA,QAAA,CAAA,QAAA,GAAA,EAAA;AACA,YAAA,IAAA,CAAA,QAAA,CAAA,IAAA,GAAA,EAAA;AACA,YAAA,IAAA,CAAA,QAAA,CAAA,SAAA,GAAA,EAAA;AACA,WALA,MAKA,IAAA,GAAA,CAAA,IAAA,IAAA,YAAA,EAAA;AACA,YAAA,KAAA,CAAA,QAAA,CAAA;AACA;AACA,SAnBA;AAqBA;AAEA,KA/BA;AAgCA,IAAA,SAhCA,qBAgCA,QAhCA,EAgCA;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,WAAA;AACA;AAlCA;AAlDA,CAAA","sourcesContent":["<!-- 登陆页面   -->\r\n<template>\r\n  <div id=\"app\">\r\n    <el-row :gutter=\"20\">\r\n      <!-- gutter 栅格间距 -->\r\n      <el-col :span=\"8\" :offset=\"8\">\r\n        <!-- span 栅格占的列数，offset是偏移列数 -->\r\n        <div class=\"grid-content\"></div>\r\n      </el-col>\r\n    </el-row>\r\n\r\n    <el-row :gutter=\"20\" style=\"padding: 25px\">\r\n      <!-- gutter 栅格间距 -->\r\n\r\n      <el-col :span=\"8\" :offset=\"8\">\r\n        <!-- span 栅格占的列数，offset是偏移列数 -->\r\n        <el-card shadow=\"always\">\r\n          <h1>用户注册</h1>\r\n          <el-divider></el-divider>\r\n\r\n          <el-form\r\n            :model=\"ruleForm\"\r\n            status-icon\r\n            :rules=\"rules\"\r\n            ref=\"ruleForm\"\r\n            label-width=\"100px\"\r\n            class=\"demo-ruleForm\"\r\n          >\r\n            <!-- 用户名 -->\r\n            <el-form-item label=\"用户名\" prop=\"username\">\r\n              <el-input\r\n                placeholder=\"请输入10字符以内的用户名\"\r\n                v-model=\"ruleForm.username\"\r\n              ></el-input>\r\n            </el-form-item>\r\n\r\n            <!-- 密码 -->\r\n            <el-form-item label=\"密码\" prop=\"pass\">\r\n              <el-input\r\n                type=\"password\"\r\n                placeholder=\"请输入密码\"\r\n                v-model=\"ruleForm.pass\"\r\n                autocomplete=\"off\"\r\n              ></el-input>\r\n            </el-form-item>\r\n            <!-- 确认密码 -->\r\n            <el-form-item label=\"确认密码\" prop=\"checkPass\">\r\n              <el-input\r\n                type=\"password\"\r\n                placeholder=\"请再次输入密码\"\r\n                v-model=\"ruleForm.checkPass\"\r\n                autocomplete=\"off\"\r\n              ></el-input>\r\n            </el-form-item>\r\n            <el-form-item>\r\n              <el-button type=\"primary\" @click=\"submitForm('ruleForm')\"\r\n                >提交</el-button\r\n              >\r\n              <el-button @click=\"resetForm('ruleForm')\">重置</el-button>\r\n            </el-form-item>\r\n          </el-form>\r\n        </el-card>\r\n      </el-col>\r\n    </el-row>\r\n  </div>\r\n</template>\r\n\r\n\r\n\r\n<script>\r\nimport {request} from '../network/request'\r\nimport Crypto from \"@/crypto/index.js\";\r\nexport default {\r\n\r\n  data() {\r\n    var checkUsername = (rule, value, callback) => {\r\n      if (!value) {\r\n        return callback(new Error(\"用户名不能为空\"));\r\n      }\r\n      setTimeout(() => {\r\n        if (value.length > 10) {\r\n          callback(new Error(\"请输入输入10字符以内的用户名\"));\r\n        } else {\r\n          callback();\r\n        }\r\n      }, 1000);\r\n    };\r\n    var validatePass = (rule, value, callback) => {\r\n      if (value === \"\") {\r\n        callback(new Error(\"请输入密码\"));\r\n      } else {\r\n        if (this.ruleForm.checkPass !== \"\") {\r\n          this.$refs.ruleForm.validateField(\"checkPass\");\r\n        }\r\n        callback();\r\n      }\r\n    };\r\n    var validatePass2 = (rule, value, callback) => {\r\n      if (value === \"\") {\r\n        callback(new Error(\"请再次输入密码\"));\r\n      } else if (value !== this.ruleForm.pass) {\r\n        callback(new Error(\"两次输入密码不一致!\"));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n    return {\r\n      userId: parseInt(localStorage.userId),\r\n      userList: [],\r\n      ruleForm: {\r\n        pass: \"\",\r\n        checkPass: \"\",\r\n        username: \"\",\r\n      },\r\n      rules: {\r\n        pass: [{ validator: validatePass, trigger: \"blur\" }],\r\n        checkPass: [{ validator: validatePass2, trigger: \"blur\" }],\r\n        username: [{ validator: checkUsername, trigger: \"blur\" }],\r\n      },\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    submitForm() {\r\n      \r\n      if(this.ruleForm.pass.length < 8){\r\n        alert(\"密码不能小于8位！\")\r\n      }else{\r\n        //发送post请求\r\n      var that = this;\r\n      request({\r\n        method: \"post\", //请求方式，默认是get请求\r\n        url: \"/register\", //地址\r\n        data: {\r\n          //这里是发送给后台的数据\r\n          username: this.ruleForm.username,\r\n          password: Crypto.set(this.ruleForm.pass),\r\n          avatar:\r\n            \"monster.jpg\",\r\n        },\r\n      }).then(function (res) {\r\n        if (res.data == \"success\") {\r\n          alert(\"注册成功！\")\r\n          that.ruleForm.username = \"\";\r\n          that.ruleForm.pass = \"\";\r\n          that.ruleForm.checkPass = \"\";\r\n        } else if (res.data == \"repetition\") {\r\n          alert(\"用户名重复！\")\r\n        }\r\n      });\r\n      \r\n      }\r\n      \r\n    },\r\n    resetForm(formName) {\r\n      this.$refs[formName].resetFields();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.content {\r\n  margin: 0 auto;\r\n}\r\n.el-card {\r\n  border-radius: 30px;\r\n  /* box-shadow: 0 2px 12px 0 rgb(243, 102, 102); */\r\n  /* box-shadow: 0 2px 4px rgba(0, 0, 0, .12), 0 0 6px rgba(0, 0, 0, .04); */\r\n}\r\n.grid-content {\r\n  /* background: rgb(14, 214, 131); */\r\n  border-radius: 4px;\r\n  min-height: 80px;\r\n}\r\n.el-row {\r\n  margin-bottom: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}