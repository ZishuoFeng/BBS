AA;AACA,MAAA,YAAA,CAAA,UAAA,GAAA,GAAA,CAAA,IAAA,CAAA,CAAA,CAAA;AACA,MAAA,YAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,CAAA,CAAA;AACA,MAAA,YAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA,CAAA,CAAA,CAAA;AACA,KAZA,EAaA,KAbA,CAaA,UAAA,GAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,KAfA;AAgBA,GAlCA;AAoCA,EAAA,OAAA,EAAA;AACA,IAAA,UADA,sBACA,MADA,EACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,IADA,GACA,IAAA,QAAA,EADA;AAEA,gBAAA,IAAA,CAAA,MAAA,CAAA,MAAA,EAAA,MAAA,CAAA,IAAA;AAFA;AAAA,uBAIA,KAAA,CAAA,MAAA,CAAA,IAAA,CACA,gCADA,EAEA,IAFA,CAJA;;AAAA;AAIA,gBAAA,GAJA;AAQA,gBAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,gBAAA,KAAA,CAAA,GAAA,GAAA,GAAA,CAAA,IAAA;;AATA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA,KAXA;AAaA,IAAA,aAbA,2BAaA;AACA,MAAA,KAAA,CAAA,MAAA,CAAA;AACA,KAfA;AAgBA,IAAA,mBAhBA,+BAgBA,GAhBA,EAgBA,IAhBA,EAgBA;AACA,WAAA,GAAA,GAAA,GAAA,CAAA,eAAA,CAAA,IAAA,CAAA,GAAA,CAAA;AACA,KAlBA;AAmBA,IAAA,kBAnBA,8BAmBA,IAnBA,EAmBA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,IAAA,KAAA,YAAA;AACA,UAAA,MAAA,GAAA,IAAA,CAAA,IAAA,GAAA,IAAA,GAAA,IAAA,GAAA,CAAA;;AAEA,UAAA,CAAA,KAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,mBAAA;AACA;;AACA,UAAA,CAAA,MAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,mBAAA;AACA;;AACA,aAAA,KAAA,IAAA,MAAA;AACA,KA9BA;AAgCA,IAAA,UAhCA,wBAgCA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA;AADA,OAAA;AAGA,KApCA;AAqCA,IAAA,YArCA,0BAqCA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,IAAA,EAAA;AADA,OAAA;AAGA;AAzCA;AApCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-tabs tab-position=\"left\" type=\"border-card\" style=\"height: 700px\">\r\n      <el-tab-pane label=\"个人信息\">\r\n        <el-row>\r\n          <el-col :span=\"8\">\r\n            <el-card :body-style=\"{ padding: '10px', margin: '20px' }\">\r\n              <!-- <img :src=\"src\" class=\"image\" /> -->\r\n              <div style=\"padding: 14px\">\r\n                <div class=\"bottom clearfix\">\r\n                  <el-upload\r\n                    class=\"avatar-uploader\"\r\n                    action=\"action\"\r\n                    :show-file-list=\"false\"\r\n                    :on-success=\"handleAvatarSuccess\"\r\n                    :before-upload=\"beforeAvatarUpload\"\r\n                    :http-request=\"uploadFile\"\r\n                    disabled\r\n                  >\r\n                    <img v-if=\"img\" :src=\"img\" class=\"avatar\" />\r\n                    <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n                    <el-button\r\n                      slot=\"tip\"\r\n                      class=\"el-upload__tip\"\r\n                      style=\"margin: 30px\"\r\n                      @click=\"changeAvatar\"\r\n                    >\r\n                      点击修改头像\r\n                    </el-button>\r\n                  </el-upload>\r\n                </div>\r\n              </div>\r\n            </el-card>\r\n          </el-col>\r\n\r\n          <el-col :span=\"16\">\r\n            <el-card :body-style=\"{ padding: '17px', margin: '20px' }\">\r\n              <div slot=\"header\" class=\"clearfix\">\r\n                <span>基本资料</span>\r\n              </div>\r\n              <div>\r\n                <el-form label-width=\"80px\" size=\"small\" label-position=\"right\">\r\n                  <el-form-item label=\"用户名\">\r\n                    {{ username }}\r\n                  </el-form-item>\r\n                  <el-form-item label=\"注册时间\">\r\n                    {{ registerTime }}\r\n                  </el-form-item>\r\n                  <el-form-item label=\"邮箱\">\r\n                    {{ email }}\r\n                  </el-form-item>\r\n                  <el-form-item label=\"发表留言\">\r\n                    {{ message }}篇\r\n                  </el-form-item>\r\n                  <el-form-item label=\"发表评论\">\r\n                    {{ comment }}条\r\n                  </el-form-item>\r\n                </el-form>\r\n              </div>\r\n            </el-card>\r\n          </el-col>\r\n        </el-row>\r\n        <!-- <el-row v-if=\"state == 0\">\r\n         <div>未登录</div>\r\n        </el-row> -->\r\n      </el-tab-pane>\r\n\r\n      <el-tab-pane label=\"修改密码\">\r\n        <el-button type=\"primary\" @click=\"changePass\">修改密码</el-button>\r\n      </el-tab-pane>\r\n\r\n      <el-tab-pane label=\"安全邮箱\">安全邮箱</el-tab-pane>\r\n    </el-tabs>\r\n  </div>\r\n</template>\r\n\r\n\r\n\r\n<script>\r\nimport {request} from '../network/request'\r\nexport default {\r\n  data() {\r\n    return {\r\n      // state: 0,\r\n      username: localStorage.username,\r\n      registerTime: localStorage.createTime,\r\n      email: \"896202576@qq.com\",\r\n      message: localStorage.message,\r\n      comment: localStorage.comment,\r\n      url: localStorage.avatar,\r\n    };\r\n  },\r\n  computed: {\r\n    img() {\r\n      return require(\"../assets/\" + this.url);\r\n    },\r\n  },\r\n  created() {\r\n    request({\r\n      method: \"post\",\r\n      url: \"http://127.0.0.1:5000/personal\",\r\n      data: {\r\n        username: localStorage.username,\r\n      },\r\n    })\r\n      .then(function (res) {\r\n          console.log(res.data);  \r\n          localStorage.createTime = res.data[0];\r\n          localStorage.message = res.data[1];\r\n          localStorage.comment = res.data[2];\r\n      })\r\n      .catch(function (err) {\r\n        console.log(err);\r\n      });\r\n  },\r\n\r\n  methods: {\r\n    async uploadFile(params) {\r\n      let form = new FormData();\r\n      form.append(\"file\", params.file);\r\n\r\n      const res = await this.$axios.post(\r\n        \"http://127.0.0.1:5000/personal\",\r\n        form\r\n      );\r\n      console.log(res);\r\n      this.img = res.data;\r\n    },\r\n\r\n    changePicture() {\r\n      alert(\"修改头像\");\r\n    },\r\n    handleAvatarSuccess(res, file) {\r\n      this.img = URL.createObjectURL(file.raw);\r\n    },\r\n    beforeAvatarUpload(file) {\r\n      const isJPG = file.type === \"image/jpeg\";\r\n      const isLt2M = file.size / 1024 / 1024 < 2;\r\n\r\n      if (!isJPG) {\r\n        this.$message.error(\"上传头像图片只能是 JPG 格式!\");\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error(\"上传头像图片大小不能超过 2MB!\");\r\n      }\r\n      return isJPG && isLt2M;\r\n    },\r\n\r\n    changePass() {\r\n      this.$router.push({\r\n        path: \"changePass\",\r\n      });\r\n    },\r\n    changeAvatar() {\r\n      this.$router.push({\r\n        path: \"changeAvatar\",\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n\r\n<style>\r\n.bottom {\r\n  margin-top: 13px;\r\n  line-height: 12px;\r\n}\r\n\r\n.image {\r\n  width: 50%;\r\n  /* display: block; */\r\n}\r\n\r\n.clearfix:before,\r\n.clearfix:after {\r\n  display: table;\r\n  content: \"\";\r\n}\r\n\r\n.clearfix:after {\r\n  clear: both;\r\n}\r\n\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409eff;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 178px;\r\n  height: 178px;\r\n  display: block;\r\n}\r\n</style>\r\n\r\n"],"sourceRoot":"src/views"}]}